	rsa/run.log \
	rsa/expexted_trimmed.log \
	rsa/run_trimmed.log \
	rsa/diff.log

examples_FILES += \
	$(rsa_H_FILES) \
	$(rsa_CXX_FILES) \
	$(rsa_BUILD) \
	$(rsa_EXTRA)

## example-specific details

rsa_H_FILES =

rsa_CXX_FILES = \
	rsa/rsa.cpp

rsa_BUILD =

rsa_EXTRA = \
	rsa/rsa.vcproj \
	rsa/Makefile \
	rsa/README

