##       names for now.
##
## Local values:
##   %D%: rsa
##   %C%: rsa

examples_TESTS += rsa/test

rsa_test_CPPFLAGS = \
	$(AM_CPPFLAGS)

rsa_test_SOURCES = \
	$(rsa_H_FILES) \
	$(rsa_CXX_FILES)

examples_BUILD += \
	$(rsa_BUILD)

