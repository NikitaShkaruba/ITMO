#include <xil_io.h>
#define GPIO_ADDRESS  0x40000000

#define TIMER_TCSR0  0x41C00000
#define TIMER_TLR0  0x41C00004
#define TIMER_TCR0  0x41C00008


//typedef unsigned int u32;

int main() {
	u32 per=10;
	Xil_Out32(TIMER_TLR0, per);
	Xil_Out32(TIMER_TCR0, 0);
	Xil_Out32(TIMER_TCSR0, /*1<<9|*/1<<7|1<<5|1<<4|1<<1);
	while(1){
		u32 v=Xil_In32(TIMER_TCR0);
		Xil_Out32(GPIO_ADDRESS, v);
	}

}
